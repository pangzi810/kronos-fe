    "@vueuse/core": "^13.7.0",
    "axios": "^1.11.0",
    "date-fns": "^4.1.0",
    "lodash-es": "^4.17.21",
    "pinia": "^3.0.3",
    "vue": "^3.5.18",
    "vue-i18n": "^12.0.0-alpha.3",
    "vue-json-pretty": "^2.5.0",
    "vue-router": "^4.5.1",
    "vue-toastification": "^2.0.0-rc.5",
    "vuetify": "^3.9.4"
